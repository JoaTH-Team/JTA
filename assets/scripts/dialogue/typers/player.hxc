import jta.Paths;
import jta.objects.dialogue.typers.Typer;

class PlayerTyper extends Typer
{
	public function new():Void
	{
		super('player');

		fontName = Paths.font('main', false);
		fontSize = 32;

		typerOffset.set(-20, -20);
		typerSounds = [{sound: Paths.sound('txt_player', false), volume: 1}];
		typerFPS = 2;
	}
}
