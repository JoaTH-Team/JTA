import jta.Paths;
import jta.objects.dialogue.typers.Typer;

class SignTyper extends Typer
{
	public function new():Void
	{
		super('sign');

		fontName = Paths.font('main', false);
		fontSize = 32;

		typerOffset.set(-20, -20);
		typerSounds = [{sound: Paths.sound('txt_sign', false), volume: 1}];
		typerFPS = 2;
	}
}
